load(sprintf('testperson.mat')); 
x = iriscode(1,:);

HDmin = 30;
person = 0;
row = 0;

for i = 1:20
    load(sprintf('person%02d.mat',i));
    for j = 1:20
       y = iriscode(j,:);
       HDtemp = 0;
       counter = 0;
       for k = 1:30
           if x(1,k) == 2
               HDtemp = HDtemp + abs(x(1,k)-y(1,k));
           end
       end
       if HDtemp < HDmin
           HDmin = HDtemp;
           person = i;
           row = j;
       end
    end
end
HDmin = HDmin/(20);

